    "welcomePage.tileBorder": "#0000001A",
    "welcomePage.tileHoverBackground": "#EDE8DD",
    "inlineChatInput.background":"#EDE8DD"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin.python"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7B776F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#426B5A"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#67978a"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#8A3A4A"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.type.function.python",
        "storage.type.class.python",
        "keyword.declaration.function",
        "keyword.declaration.class"
      ],
      "settings": {
        "foreground": "#E0734A"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "entity.name.operator.custom-literal"
      ],
      "settings": {
        "foreground": "#5B4A2F"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#3E6170"
      }
    },
    {
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "entity.name.variable"
      ],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#3E6170"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#B55333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#2EA043"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#D84B38"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#E0734A"
      }
    },
    {
      "scope": [
        "punctuation.definition.tag",
        "entity.name.tag",
        "entity.name.selector"
      ],
      "settings": {
        "foreground": "#5B4A2F"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#B55333"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#D84B38"
      }
    }
  ]
}