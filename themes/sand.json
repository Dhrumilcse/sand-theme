    "tab.activeForeground": "#1A1A1A",
    "tab.activeBorderTop": "#E0734A",
    "tab.inactiveBackground": "#F2EFE8",
    "tab.inactiveForeground": "#7B776F",
    "tab.hoverBackground": "#FFFFFF",
    "tab.selectedBackground": "#FFFFFFA5",
